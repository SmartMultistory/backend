<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE logback>
<configuration>
<appender  name="STDOUT" class="ch.gos.logback.core.ConsoleAppender">
    <encoder>
        <pattern>%data{HH:mm:ss.SSS} [%thread] %-5level %logger{15}#%line %msg\n</pattern>
    </encoder>
</appender>
    <logger name="net.proselyte.springsecurityapp">
        <level value="error"/>
        <appender-ref ref="STDOUT"/>

    </logger>
</configuration>